<Window
    x:Class="FFTDemoApp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:FFTDemoApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <oxy:Plot oxy:Name="plotmodel">
            <oxy:Plot.Series>
                <oxy:LineSeries oxy:Name="line1" />
            </oxy:Plot.Series>
            <oxy:Plot.Axes>
                <oxy:LinearAxis
                    Name="yAxes"
                    Title="Y"
                    AxislineColor="Gray"
                    AxislineStyle="Solid"
                    IntervalLength="150"
                    MajorGridlineColor="Gray"
                    MajorGridlineStyle="Solid"
                    MinorGridlineColor="Gray"
                    MinorGridlineStyle="Dot"
                    Position="Left"
                    StartPosition="-0.01"
                    TextColor="Gray"
                    TickStyle="None"
                    TitleColor="Gray" />
            </oxy:Plot.Axes>
        </oxy:Plot>
    </Grid>
</Window>
